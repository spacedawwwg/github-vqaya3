---
import Layout from "../layouts/Layout.astro";
import Example from "../components/Example.vue";

const items = [
	{
		id: "1",
	},
	{
		id: "2",
	},
	{
		id: "abc",
	},
];
---

<Layout title="Welcome to Astro.">
		<Example items={items}>
			{items.map(({ id }) => <p slot={id}>{id}</p>)}
		</Example>
</Layout>